<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD config.mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ssm.dao.CardAccountDao">

	<select id="findAllCardAccounts" resultType="com.ssm.entity.CardAccount">
	SELECT *
	FROM card_accounts
	</select>

	<select id="findCardAccountById" resultType="com.ssm.entity.CardAccount">
		SELECT *
		FROM card_accounts
		WHERE account_id = #{id}
	</select>
</mapper>
